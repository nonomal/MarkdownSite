<!-- Begin dashboard/_website/milestone -->
<div class="card mb-4">
    <div class="card-header">
%       if ( $milestone->{is_complete} == 1 ) {
            <span class="text-success" >
                %=  include '_svg/checkmark'
            </span>
%       } else {
            %= include '_svg/loading'
%       }
        <%= $milestone->{text} %>
    </div>
</div>
<!-- End dashboard/_website/milestone -->

